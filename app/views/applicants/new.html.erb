<h1>Pet Adoption Form</h1>
<%= form_with url: "/applications/:applicant_id", method: :post, local: true do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :address %>
<%= f.text_field :address  %>
<%= f.label :zip %>
<%= f.text_field :zip %>
<%= f.label :city %>
<%= f.text_field :city %>
<%= f.label :state %>
<%= f.text_field :state %>
<%= f.label :description %>
<%= f.text_field :description %>
<%= f.submit %>
<% end %>